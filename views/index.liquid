{% render 'partials/head.liquid' %}

<article>
  <section class="front">
    <h1>2026 PROFILE CARD</h1>

    <img class="front-photo" src="/assets/mugshot-za.png" alt="pasfoto Seb" width="240">
    
    <ul>
      <li>
        <label>[Name]</label>
        {{person.name}}
      </li>
      <li>
        <label>[Born]</label>
        {{person.birthdate}}
      </li>
      <li>
        <label>[Length]</label>
        {{custom.length}}cm
      </li>
      <li>
        <label>[Based in]</label>
        {{custom.based}}
      </li>
      <li>
        <label>[Studying at]</label>
        {{custom.studying}}
      </li>
    </ul>
  </section>

  <section class="back">
    <img class="back-photo" src="/assets/schoolfotoYN.png" alt="oude foto van Seb" width="200">
  </section>
</article>
{% render 'partials/foot.liquid' %}
